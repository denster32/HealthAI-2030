<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- CloudKit Configuration with iOS 18+ Features -->
    <key>com.apple.developer.icloud-services</key>
    <array>
        <string>CloudKit</string>
        <string>CloudDocuments</string>
    </array>
    <key>com.apple.developer.icloud-container-identifiers</key>
    <array>
        <string>iCloud.com.healthai2030.HealthAI2030</string>
    </array>
    <key>com.apple.developer.icloud-container-development-container-identifiers</key>
    <array>
        <string>iCloud.com.healthai2030.HealthAI2030.development</string>
    </array>

    <!-- Enhanced HealthKit with iOS 18+ Features -->
    <key>com.apple.developer.healthkit</key>
    <true/>
    <key>com.apple.developer.healthkit.access</key>
    <array>
        <string>health-records</string>
    </array>
    <key>com.apple.developer.healthkit.background-delivery</key>
    <true/>
    <key>com.apple.developer.healthkit.recalibrate-estimates</key>
    <true/>

    <!-- App Groups for Enhanced Cross-Device Sync -->
    <key>com.apple.security.application-groups</key>
    <array>
        <string>group.com.healthai2030.shared</string>
        <string>group.com.healthai2030.widgets</string>
        <string>group.com.healthai2030.notifications</string>
        <string>group.com.healthai2030.shortcuts</string>
        <string>group.com.healthai2030.watch</string>
        <string>group.com.healthai2030.ml</string>
        <string>group.com.healthai2030.graphics</string>
        <string>group.com.healthai2030.privacy</string>
    </array>

    <!-- Enhanced Background Processing for iOS 18+ -->
    <key>com.apple.developer.background-modes</key>
    <array>
        <string>background-fetch</string>
        <string>background-processing</string>
        <string>remote-notification</string>
        <string>background-app-refresh</string>
        <string>bluetooth-central</string>
        <string>bluetooth-peripheral</string>
        <string>external-accessory</string>
        <string>location</string>
        <string>audio</string>
        <string>health-kit</string>
    </array>

    <!-- Enhanced Keychain Sharing -->
    <key>keychain-access-groups</key>
    <array>
        <string>$(AppIdentifierPrefix)com.healthai2030.HealthAI2030</string>
        <string>$(AppIdentifierPrefix)group.com.healthai2030.shared</string>
        <string>$(AppIdentifierPrefix)group.com.healthai2030.widgets</string>
        <string>$(AppIdentifierPrefix)group.com.healthai2030.notifications</string>
        <string>$(AppIdentifierPrefix)group.com.healthai2030.shortcuts</string>
        <string>$(AppIdentifierPrefix)group.com.healthai2030.watch</string>
        <string>$(AppIdentifierPrefix)group.com.healthai2030.ml</string>
        <string>$(AppIdentifierPrefix)group.com.healthai2030.privacy</string>
    </array>

    <!-- Push Notifications with iOS 18+ Features -->
    <key>com.apple.developer.aps-environment</key>
    <string>development</string>
    
    <!-- Network Extensions -->
    <key>com.apple.developer.networking.networkextension</key>
    <array>
        <string>app-proxy-provider</string>
        <string>content-filter-provider</string>
    </array>

    <!-- Enhanced Core ML and Neural Engine Access -->
    <key>com.apple.developer.coreml.private-model-access</key>
    <true/>
    <key>com.apple.developer.coreml.allow-model-execution</key>
    <true/>

    <!-- Metal Performance Shaders with iOS 18+ -->
    <key>com.apple.developer.metal</key>
    <true/>
    <key>com.apple.developer.metal.performance-shaders.graph</key>
    <true/>

    <!-- Associated Domains -->
    <key>com.apple.developer.associated-domains</key>
    <array>
        <string>applinks:healthai2030.com</string>
        <string>applinks:sync.healthai2030.com</string>
        <string>applinks:api.healthai2030.com</string>
        <string>webcredentials:healthai2030.com</string>
        <string>activitycontinuation:healthai2030.com</string>
    </array>

    <!-- Enhanced Siri Integration -->
    <key>com.apple.developer.siri</key>
    <true/>
    <key>com.apple.developer.siri.intent-confirmation</key>
    <true/>

    <!-- App Clips Configuration -->
    <key>com.apple.developer.parent-application-identifiers</key>
    <array>
        <string>$(AppIdentifierPrefix)com.healthai2030.HealthAI2030</string>
    </array>

    <!-- iOS 18+ Privacy and Security Features -->
    <key>com.apple.developer.family-controls</key>
    <true/>
    
    <key>com.apple.developer.focus-status</key>
    <true/>
    
    <key>com.apple.developer.usernotifications.time-sensitive</key>
    <true/>
    
    <key>com.apple.developer.usernotifications.critical-alerts</key>
    <true/>

    <!-- App Attest for Security -->
    <key>com.apple.developer.devicecheck.appattest-environment</key>
    <string>development</string>

    <!-- iOS 18+ Live Activities -->
    <key>com.apple.developer.ActivityKit</key>
    <true/>

    <!-- iOS 18+ Speech Recognition with Privacy -->
    <key>com.apple.developer.speech-recognition</key>
    <true/>

    <!-- iOS 18+ SensitiveContentAnalysis -->
    <key>com.apple.developer.sensitivecontentanalysis.client</key>
    <true/>

    <!-- iOS 18+ Advanced Permissions -->
    <key>com.apple.developer.contacts.notes</key>
    <true/>
    
    <key>com.apple.developer.exposure-notification</key>
    <true/>

    <!-- iOS 18+ Screen Time API -->
    <key>com.apple.developer.screen-time</key>
    <true/>

    <!-- iOS 18+ Game Center -->
    <key>com.apple.developer.game-center</key>
    <true/>

    <!-- iOS 18+ Maps -->
    <key>com.apple.developer.maps</key>
    <true/>

    <!-- iOS 18+ WeatherKit -->
    <key>com.apple.developer.weatherkit</key>
    <true/>

    <!-- iOS 18+ App Store Server API -->
    <key>com.apple.developer.storekit.external-purchase-link</key>
    <true/>

    <!-- iOS 18+ ClassKit -->
    <key>com.apple.developer.ClassKit-environment</key>
    <string>development</string>

    <!-- iOS 18+ AutoFill -->
    <key>com.apple.developer.authentication-services.autofill-credential-provider</key>
    <true/>

    <!-- iOS 18+ Wallet and Apple Pay -->
    <key>com.apple.developer.payment-pass-provisioning</key>
    <true/>
    
    <key>com.apple.developer.in-app-payments</key>
    <array>
        <string>merchant.com.healthai2030.payments</string>
    </array>

    <!-- iOS 18+ CarPlay -->
    <key>com.apple.developer.carplay-audio</key>
    <true/>
    
    <key>com.apple.developer.carplay-communication</key>
    <true/>

    <!-- iOS 18+ Matter and HomeKit -->
    <key>com.apple.developer.homekit</key>
    <true/>
    
    <key>com.apple.developer.matter.allow-setup-payload</key>
    <true/>

    <!-- iOS 18+ Group Activities -->
    <key>com.apple.developer.group-session</key>
    <true/>

    <!-- iOS 18+ SharePlay -->
    <key>com.apple.developer.shared-with-you</key>
    <true/>

    <!-- iOS 18+ App Tracking Transparency -->
    <key>com.apple.developer.tracking-domains</key>
    <array>
        <string>healthai2030.com</string>
    </array>

    <!-- iOS 18+ Enhanced Networking -->
    <key>com.apple.developer.networking.wifi-info</key>
    <true/>
    
    <key>com.apple.developer.networking.multicast</key>
    <true/>

    <!-- iOS 18+ Advanced Media -->
    <key>com.apple.developer.avfoundation.multichannelaudiosessionmode</key>
    <true/>
    
    <key>com.apple.developer.coremedia.hls.low-latency</key>
    <true/>

    <!-- iOS 18+ Research and Health Studies -->
    <key>com.apple.developer.healthkit.research</key>
    <true/>

    <!-- iOS 18+ Developer Mode Support -->
    <key>com.apple.developer.kernel.increased-memory-limit</key>
    <true/>
</dict>
</plist>